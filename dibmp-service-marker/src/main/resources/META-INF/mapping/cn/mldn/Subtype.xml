<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mldn.dibmp.dao.ISubtypeDAO">
	<!-- 查询所有的仓库类型信息 -->
	<select id="findByWiid" parameterType="Long" resultType="Subtype">
		SELECT stid,title,wiid FROM subtype WHERE wiid=#{wiid}
	</select>
</mapper>